    "string-raw": "^1.0.1"
  },
  "devDependencies": {
    "@storybook/addons": "^3.2.15",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  },
  "files": [
    "components",
    "env",
    "services",
